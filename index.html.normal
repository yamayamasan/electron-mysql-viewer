</head>
<body>
  <section id="start">
    <div id="connectionList">
      <ul>
        <li id="conList01"><a onClick="test01()">test01</a></li>
        <li id="conList02"></li>
      </ul>
    </div>
    
    <div id="newConnection">
    <label for='connectionName'>name</label>
    <input type='text' name='connectionName' id='connectionName'>
    <br />
    <label for='host'>host</label>
    <input type='text' name='host' id='host' />
    <br />
    <label for='user'>user</label>
    <input type='text' name='user' id='user' />
    <br />
    <label for='password'>password</label>
    <input type='text' name='password' id='password' />
    <br />
    <label for='db'>db</label>
    <input type='text' name='db' id='db' />
    <br />
    <button onClick="dbTestConnect()">Test Connect</button>
    <button onClick="saveDbOptions()">save</button>
    </div>
  </section>

  <section id="main">
      <div id="operation">
        <div id="editor" style="height: 50px"></div>
        <button onClick="runQuery()">run</button>
      </div>
    
      <div id='result'></div>

      <div id="queryLog"></div>
  </section>
